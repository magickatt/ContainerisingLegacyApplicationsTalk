{
  "Node": "database-mysql",
  "Address": "database-mysql.default.svc.cluster.local",
  "NodeMeta": {
    "external-node": "true",
    "external-probe": "true"
  },
  "Service": {
    "ID": "database",
    "Service": "database",
    "Port": 3306
  },
  "Checks": [{
    "name": "ping check",
    "args": ["ping","-c1","database-mysql.default.svc.cluster.local"],
    "interval": "30s",
    "status": "passing"
  }]
}

